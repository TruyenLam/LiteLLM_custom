# LiteLLM Environment Variables for Railway Deployment
# Copy this file to .env and fill in your actual values

# AIMLAPI Configuration (Primary Provider)
AIMLAPI_KEY = "your_aimlapi_key_here"

# OpenAI (Optional - if you have direct OpenAI access)
OPENAI_API_KEY = ""
OPENAI_BASE_URL = ""

# Cohere (Optional)
COHERE_API_KEY = ""

# OpenRouter (Optional)
OR_SITE_URL = ""
OR_APP_NAME = "LiteLLM Example app"
OR_API_KEY = ""

# Azure API (Optional)
AZURE_API_BASE = ""
AZURE_API_VERSION = ""
AZURE_API_KEY = ""

# Replicate (Optional)
REPLICATE_API_KEY = ""
REPLICATE_API_TOKEN = ""

# Anthropic (Optional - AIMLAPI provides Claude models)
ANTHROPIC_API_KEY = ""

# Infisical (Optional)
INFISICAL_TOKEN = ""

# Novita AI (Optional)
NOVITA_API_KEY = ""

# INFINITY (Optional)
INFINITY_API_KEY = ""

# LiteLLM Core Configuration
LITELLM_MASTER_KEY = "sk-your-chosen-master-key-here"
LITELLM_SALT_KEY = "sk-your-salt-key-here"

# Database Configuration (Required for Railway)
DATABASE_URL = "postgresql://username:password@host:port/database_name"
STORE_MODEL_IN_DB = "True"

# Optional: Redis for caching
# REDIS_HOST = "your_redis_host"
# REDIS_PORT = "6379"  
# REDIS_PASSWORD = "your_redis_password"
